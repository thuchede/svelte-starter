{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "watch:tailwind": "postcss static/tailwind.css -o static/index.css -w",
    "build:tailwind": "NODE_ENV=production postcss static/tailwind.css -o static/index.css",
    "dev": "run-p watch:tailwind routify rollup",
    "dev-dynamic": "cross-env BUNDLING=dynamic npm run dev",
    "build": "npm run watch:tailwind && routify -bD && rollup -c",
    "serve": "spassr --serve-spa --serve-ssr",
    "export": "routify export",
    "deploy:now": "cd scripts/now && npm run deploy",
    "deploy:netlify": "cd scripts/netlify && npm run deploy",
    "rollup": "rollup -cw",
    "routify": "routify -D"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.2",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "del": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "ppid-changed": "^1.0.1",
    "rollup": "^2.6.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-livereload": "^1.2.0",
    "rollup-plugin-svelte": "^5.2.1",
    "rollup-plugin-terser": "^5.3.0",
    "spassr": "^1.0.2",
    "tailwindcss": "^1.4.4"
  },
  "dependencies": {
    "@sveltech/routify": "^1.5.9",
    "svelte": "^3.20.1"
  }
}
